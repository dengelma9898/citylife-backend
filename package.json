{
  "name": "citylife-backend",
  "version": "1.0.0",
  "description": "Backend for Citylife App",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:prd": "NODE_ENV=prd nest build",
    "build:dev": "NODE_ENV=dev nest build",
    "format": "prettier --write \"src/**/*.ts\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"src/**/*.ts\" --ignore-path .prettierignore",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prd": "NODE_ENV=prd nest start",
    "start:dev": "NODE_ENV=dev nest start",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "sonar": "sonar-scanner",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.17",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.17",
    "@nestjs/platform-express": "^11.0.17",
    "@nestjs/swagger": "^11.1.2",
    "@types/adm-zip": "^0.5.7",
    "adm-zip": "^0.5.16",
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "firebase": "^11.6.0",
    "firebase-admin": "^13.2.0",
    "luxon": "^3.6.1",
    "node-fetch": "^2.7.0",
    "passkit-generator": "^3.3.1",
    "puppeteer": "^24.9.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.0.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.6.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "@types/node-fetch": "^2.6.12",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.30.0",
    "@typescript-eslint/parser": "^8.30.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.5.3",
    "sonarqube-scanner": "^2.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
